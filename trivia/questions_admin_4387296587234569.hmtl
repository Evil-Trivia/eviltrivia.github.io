<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Admin Redirect - Last Week's Trivia</title>
  <script type="text/javascript">
    document.addEventListener('DOMContentLoaded', function() {
      // Fetch the latest lastweekstrivia page
      fetch('/lastweekstrivia*.html')
        .then(response => response.text())
        .then(html => {
          // Create a DOM parser to extract the latest filename from the response
          const parser = new DOMParser();
          const doc = parser.parseFromString(html, "text/html");
          const latestPageLink = doc.querySelector("a[href^='/lastweekstrivia']").getAttribute("href");
          
          // Redirect to the latest page after 20 seconds
          setTimeout(function() {
            window.location.href = latestPageLink;
          }, 20000); // 20 seconds delay
        })
        .catch(error => {
          console.error('Error fetching the latest trivia page:', error);
        });
    });
  </script>
</head>
<body>
  <h1>Admin Redirect - Last Week's Trivia</h1>
  <p>Loading the latest trivia page... You will be redirected in 20 seconds.</p>
</body>
</html>
