<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Admin Redirect - Last Week's Trivia</title>
  <script type="text/javascript">
    document.addEventListener('DOMContentLoaded', function() {
      fetch('/latest_pages.json')
        .then(response => response.json())
        .then(data => {
          const triviaPageUrl = data.lastweekstrivia;
          setTimeout(function() {
            window.location.href = triviaPageUrl;
          }, 20000); // 20 seconds delay
        })
        .catch(error => {
          console.error('Error fetching the latest trivia page:', error);
        });
    });
  </script>
</head>
<body>
  <h1>Admin Redirect - Last Week's Trivia</h1>
  <p>Loading the latest trivia page... You will be redirected in 20 seconds.</p>
</body>
</html>
